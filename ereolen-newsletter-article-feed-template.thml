<!-- Start template for article feed -->

<table align="left" width="100%" border="0" cellpadding="0" cellspacing="0" class="ereolTextContent" style="min-width:100%; background-color: #ffffff;" id="{nid}">
    <tbody>
        <tr>
            <td valign="top" style="padding-left: 9px; padding-top: 9px; padding-bottom: 0px;">
                <h3 style="font-size: 22px; padding-bottom: 0px;"><a href="{content url}" style="color: #007AFF; text-decoration: none;">{title}</a></h3>
            </td>
            <td width="39">
                <img src="ereolen-newletter-assets/icon-badge-book@2x.png" alt="" style="padding-right: 0px; display: inline;padding-top: 9px;" height="30" width="30"/>
            </td>
            <td width="39">
                <img src="ereolen-newletter-assets/icon-badge-sound@2x.png" alt="" style="padding-right: 0px; display: inline;padding-top: 9px;" height="30" width="30"/>
            </td>
            <td rowspan="2" width="140">
                <img src="{ImageUrl}" alt="" style="padding-right: 9px; padding-top: 9px;" height="auto" width="140"/>
            </td>
        </tr>
        <tr>
            <td valign="top" colspan="3" style="padding-left: 9px; padding-right: 9px;" >
                <p>{Description}</p>
            </td>
        </tr>
        <tr>
            <td valign="top" style="padding-left: 9px; padding-bottom: 9px;">
                <a href="{content url}" style="color: #007AFF;">LÃ¦s mere her</a>
            </td>
        </tr>
    </tbody>
</table>

<!-- End template for article feed -->